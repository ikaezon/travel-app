{
  "expo": {
    "name": "travel-app",
    "slug": "travel-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#18181b"
    },
    "ios": {
      "supportsTablet": true,
      "backgroundColor": "#18181b"
    },
    "android": {
      "backgroundColor": "#18181b"
    },
    "plugins": [
      "expo-font",
      "@react-native-community/datetimepicker",
      [
        "expo-system-ui",
        {
          "backgroundColor": "#18181b"
        }
      ]
    ]
  }
}
